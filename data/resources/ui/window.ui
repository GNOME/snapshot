<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="Window" parent="AdwApplicationWindow">
    <property name="width-request">360</property>
    <property name="height-request">294</property>
    <child>
      <object class="AdwBreakpoint">
        <condition>max-aspect-ratio: 4/3 or max-height: 600</condition>
        <setter object="camera" property="breakpoint">SingleHorizontal</setter>
      </object>
    </child>
    <child>
      <object class="AdwBreakpoint">
        <condition>max-height: 400</condition>
        <setter object="camera" property="breakpoint">DualVertical</setter>
      </object>
    </child>
    <child>
      <object class="AdwBreakpoint">
        <condition>max-aspect-ratio: 4/3 and max-width: 500</condition>
        <setter object="camera" property="breakpoint">DualHorizontal</setter>
      </object>
    </child>
    <child>
      <object class="AdwToastOverlay" id="toast_overlay">
        <child>
          <object class="AdwLeaflet" id="leaflet">
            <property name="vexpand">True</property>
            <property name="can-unfold">False</property>
            <property name="can-navigate-back">True</property>
            <child>
              <object class="Camera" id="camera">
              </object>
            </child>
            <child>
              <object class="Gallery" id="gallery">
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
